{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_API_URL": "https://${VERCEL_URL}",
    "NEXT_PUBLIC_WS_URL": "wss://${VERCEL_URL}"
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": true
    }
  }
} 