{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "API_URL": "https://api.tradingbot.com",
    "WS_URL": "wss://ws.tradingbot.com"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 